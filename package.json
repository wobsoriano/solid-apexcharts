{
  "name": "solid-apexcharts",
  "type": "module",
  "version": "0.1.6",
  "private": false,
  "packageManager": "pnpm@7.5.0",
  "description": "Solid component for ApexCharts",
  "author": "Robert Soriano",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/solid-apexcharts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/solid-apexcharts.git"
  },
  "bugs": {
    "url": "https://github.com/wobsoriano/solid-apexcharts/issues"
  },
  "keywords": [
    "solid"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "solid": "./dist/source/index.jsx",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "browser": {
        "require": "./dist/cjs/index.js",
        "import": "./dist/esm/index.js"
      },
      "node": "./dist/cjs/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "playground:dev": "vite serve playground",
    "playground:preview": "vite preview playground",
    "playground:build": "vite build playground",
    "test": "echo \"test not setup\"",
    "build": "rollup -c",
    "prepublishOnly": "pnpm build",
    "typecheck": "tsc --noEmit",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "apexcharts": ">=3.36.0",
    "solid-js": ">=1.0.0"
  },
  "dependencies": {
    "defu": "^6.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.27.0",
    "apexcharts": "^3.36.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.26.0",
    "eslint-plugin-solid": "^0.7.4",
    "rollup": "^2.77.2",
    "rollup-preset-solid": "^1.4.0",
    "solid-apexcharts": "workspace:*",
    "solid-js": "^1.4.8",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vite-plugin-solid": "^2.3.0"
  }
}
