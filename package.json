{
  "name": "solid-apexcharts",
  "type": "module",
  "version": "0.3.2",
  "private": false,
  "packageManager": "pnpm@8.6.0",
  "description": "Solid component for ApexCharts",
  "author": "Robert Soriano",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/solid-apexcharts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/solid-apexcharts.git"
  },
  "bugs": {
    "url": "https://github.com/wobsoriano/solid-apexcharts/issues"
  },
  "keywords": [
    "solid",
    "charts"
  ],
  "sideEffects": false,
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      }
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "typesVersions": {},
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.6.0"
  },
  "scripts": {
    "dev": "vite serve dev",
    "test": "echo \"test not setup\"",
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "lint": "concurrently pnpm:lint:*",
    "lint:code": "eslint --ignore-path .gitignore --max-warnings 0 src/**/*.{js,ts,tsx,jsx}",
    "lint:types": "tsc --noEmit"
  },
  "peerDependencies": {
    "apexcharts": "^3.40.0",
    "solid-js": "^1.6.0"
  },
  "dependencies": {
    "defu": "^6.1.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "apexcharts": "^3.45.1",
    "bumpp": "^9.2.1",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.11",
    "esbuild-plugin-solid": "^0.5.0",
    "eslint": "^8.45.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-solid": "^0.13.0",
    "prettier": "3.0.0",
    "solid-js": "^1.8.7",
    "tsup": "^8.0.1",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-solid": "^2.8.0"
  }
}
