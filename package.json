{
  "name": "solid-apexcharts",
  "type": "module",
  "version": "0.2.4",
  "private": false,
  "packageManager": "pnpm@8.6.1",
  "description": "Solid component for ApexCharts",
  "author": "Robert Soriano",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/solid-apexcharts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/solid-apexcharts.git"
  },
  "bugs": {
    "url": "https://github.com/wobsoriano/solid-apexcharts/issues"
  },
  "keywords": [
    "solid"
  ],
  "sideEffects": false,
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "require": "./dist/dev.cjs",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      }
    },
    "require": "./dist/index.cjs",
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "typesVersions": {},
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"test not setup\"",
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "apexcharts": "^3.40.0",
    "solid-js": "^1.6.0"
  },
  "dependencies": {
    "defu": "^6.1.2"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.39.5",
    "apexcharts": "^3.40.0",
    "bumpp": "^9.1.1",
    "esbuild": "^0.17.19",
    "esbuild-plugin-solid": "^0.5.0",
    "eslint": "^8.42.0",
    "eslint-plugin-solid": "^0.12.1",
    "solid-js": "^1.7.6",
    "tsup": "^6.7.0",
    "tsup-preset-solid": "^0.1.8",
    "typescript": "^5.1.3"
  }
}
