{
  "name": "solid-apexcharts",
  "type": "module",
  "version": "0.3.2",
  "private": false,
  "packageManager": "pnpm@8.10.2",
  "description": "Solid component for ApexCharts",
  "author": "Robert Soriano",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/solid-apexcharts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/solid-apexcharts.git"
  },
  "bugs": {
    "url": "https://github.com/wobsoriano/solid-apexcharts/issues"
  },
  "keywords": [
    "solid",
    "charts"
  ],
  "sideEffects": false,
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      }
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "typesVersions": {},
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"test not setup\"",
    "build": "tsup",
    "prepublishOnly": "pnpm build",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "apexcharts": "^3.40.0",
    "solid-js": "^1.6.0"
  },
  "dependencies": {
    "defu": "^6.1.3"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.43.1",
    "@types/node": "^20.10.5",
    "apexcharts": "^3.44.0",
    "bumpp": "^9.2.0",
    "esbuild": "^0.19.5",
    "esbuild-plugin-solid": "^0.5.0",
    "eslint": "^8.53.0",
    "eslint-plugin-solid": "^0.13.0",
    "solid-js": "^1.8.7",
    "tsup": "^7.2.0",
    "tsup-preset-solid": "^2.1.0",
    "typescript": "^5.2.2"
  }
}
